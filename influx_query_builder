CREATE CONTINUOUS QUERY "water_temp_query" ON "Schwandner_DB" BEGIN SELECT mean("water_temp") as "water_temp_mean", max("water_temp") as "water_temp_max", min("water_temp") as "_water_tempmin", count("water_temp") as "water_temp_count" INTO "data_mind_1h" FROM "data_mind" GROUP BY time(1d), "sensor_id" END
